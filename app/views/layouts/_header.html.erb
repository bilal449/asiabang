<header>
  <div class="navbar navbar-inverse navbar-static-top">
    <div class="navbar-inner">
      <div class="container">
        <button class="btn btn-navbar" data-target=".nav-collapse" data-toggle="collapse" type="button">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <div class="nav-collapse collapse">
          <ul class="nav">
            <li>
              <%= link_to root_path do %>
                <i class="icon-home"></i>
              <% end %>
            </li>
            <li><%= link_to "About us", about_us_path %></li>
            <li><%= link_to "Contact us", contact_us_path %></li>
          </ul>
        </div>
        <!--/.nav-collapse -->
      </div>
    </div>
  </div>
  <div id="logo-container">
    <div class="container">
      <div class="row">
        <div class="span8">
          <%= link_to (image_tag 'logo.png'), root_path, class: "brand logo-text" %>
          <span class="tag-line">Asian Massage Around the world..</span>
        </div>
        <div class="span4">
          <ul class="inline pull-right">
            <% if user_signed_in? %>
              <%= link_to "Rediger", edit_user_registration_path %> &nbsp   &nbsp <%= link_to "Log ud", destroy_user_session_path, method: :delete  %>
            <% else %>
              <li>
              <%= link_to "Log ind", new_user_session_path %> &nbsp   &nbsp <%= link_to "Registrer", new_user_registration_path %>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
  </div>
  <!-- #logo-container -->
</header>  